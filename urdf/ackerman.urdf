      <inertia ixx="0.0006770833333333335" ixy="0" ixz="0" iyy="0.0006770833333333335" iyz="0" izz="0.0012500000000000002"/>
    </inertial>
  </link>
  <!-- LINK VOLANTE-->
  <link name="steering_wheel">
