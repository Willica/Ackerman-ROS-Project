    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.0006770833333333335" ixy="0" ixz="0" iyy="0.0006770833333333335" iyz="0" izz="0.0012500000000000002"/>
    </inertial>
  </link>
  <!-- LINK RUEDA TRASERA IZQUIERDA -->
  <link name="left_wheel_rear">
    <visual>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="11"/>
      <inertia ixx="0.02979166666666667" ixy="0" ixz="0" iyy="0.02979166666666667" iyz="0" izz="0.05500000000000001"/>
    </inertial>
  </link>
  <!-- LINK RUEDA TRASERA DERECHA -->
  <link name="right_wheel_rear">
    <visual>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="11"/>
      <inertia ixx="0.02979166666666667" ixy="0" ixz="0" iyy="0.02979166666666667" iyz="0" izz="0.05500000000000001"/>
    </inertial>
  </link>
  <!-- LINK RUEDA FRONTAL IZQUIERDA -->
  <link name="left_wheel_front">
    <visual>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="11"/>
      <inertia ixx="0.02979166666666667" ixy="0" ixz="0" iyy="0.02979166666666667" iyz="0" izz="0.05500000000000001"/>
    </inertial>
  </link>
  <!-- LINK RUEDA FRONTAL DERECHA -->
  <link name="right_wheel_front">
    <visual>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <sphere radius="0.1"/>
